<script>
    $('.submit_application').click(function () {
       var data ={
           customer_name:$('.customer_name').val(),
           category:$('.category1').val(),
           customer_company:$('.customer_company').val(),
           customer_phone:$('.customer_phone').val(),
           customer_mail:$('.customer_mail').val(),
           industry:$('.industry').val(),
           all_ids:$('.all_ids').val()
       };
        $.ajax({
            type: "POST",
            url: "../copyright/shopAuthorization",
            data: data,
            dataType: 'json',
            success: function (data) {
                if (data.status == -10086) {
                    alert(data.msg);
                    return false;
                }else if (data.status == -1) {
                    alert(data.msg);
                    return false;
                } else if (data.status == 1) {
                    alert("申请成功，稍后有搜猎人工作人员联系您！");
                    location.reload();
                    return true;
                }
            }
        })
    });
    $('.div_error').click(function () {
         $('.div_random_bj').hide();
         $('.div_random').hide();
    });
    $('.application').click(function () {
         $('.div_random_bj').show();
         $('.div_random').show();
    });

    var price = $('.price');
    $.each(price,function (index,dom) {
        var ee =$(dom).html();
        if(ee=='￥-1.00'){
            $(dom).empty();
            $(dom).html('议价');
        }else if(ee=='￥-2.00'){
            $(dom).empty();
            $(dom).html('非卖品');
        }else if(ee=='￥-3.00'){
            $(dom).empty();
            $(dom).html('已售');
        }else if(ee=='￥-4.00'){
            $(dom).empty();
            $(dom).html('展示');
        }
    });
</script>
<script>
    var session = $('#session').val();
    $('.gerenzhongxin').click(function () {
        if (session == '') {
            alert("请登录");
        } else {
            window.location.href = "/Home/Personal/order.html";
        }
    });
    $('.dianpu').click(function () {
        if (session == '') {
            alert("请登录");
        } else {
            window.location.href = "/Home/SellerCenter/index.html";
        }
    });
    $('.woguanlidedianpu').click(function () {
        if (session == '') {
            alert("请登录");
        } else {
            window.location.href = "/Home/shop/";
        }
    });
    var swiper = new Swiper('.swiper-container', {
        pagination: '.swiper-pagination',
        paginationClickable: '.swiper-pagination',
        spaceBetween: 30,
        autoplay: 3500,
        autoplayDisableOnInteraction: false
//            loop:true
    });
    var $banner = $('.banner'),
        $bImgLen = $banner.find('img').length;
    if ($bImgLen == 0) {
        $banner.remove();
    } else if ($bImgLen == 1) {
        $banner.find('.swiper-pagination').hide();
    }
</script>
