    });
    var $banner = $('.banner'),
        $bImgLen = $banner.find('img').length;
    if ($bImgLen == 0) {
        $banner.remove();
    } else if ($bImgLen == 1) {
        $banner.find('.swiper-pagination').hide();
    }
</script>
