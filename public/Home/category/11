<script src="/PcTpl/Public/js/ImgLoad.js"></script>
<script>
    var img = $('.thumb');
    $.each(img,function (index,dom) {
    var src = $(this).next('.s_img').val();
    console.log(src);
    $(dom).attr("src",""+src);
    });
</script>
<script>
    var price = $('.price');
   $.each(price,function (index,dom) {
       var ee =$(dom).html();
       if(ee=='-1.00'){
           $(dom).empty();
           $(dom).html('议价');
       }else if(ee=='-2.00'){
           $(dom).empty();
           $(dom).html('非卖品');
       }else if(ee=='-3.00'){
           $(dom).empty();
           $(dom).html('已售');
       }else if(ee=='-4.00'){
           $(dom).empty();
           $(dom).html('展示');
       }
   });
    console.log();
    $('.favorite').click(function () {
        var goods_id = $(this).find('.goods_id').val();
        var _this = $(this);
        $.ajax({
            type: "POST",
            url: "/Home/customerCollection/customer_collection.html",
            data: {goods_id: goods_id},
            success: function (data) {
                var aaa = JSON.parse(data);
                if (aaa.status == 1) {
                    _this.find('.heart-shape').removeClass('heart-shape').addClass('heart-change');
                    alert(aaa.msg);
                } else {
                    $('.loginBtn').click();
                }
            }
        });

    });
    $('.allGoodsList>ul>li').mouseenter(function () {
        $(this).find('.bubble').fadeIn(200);
    }).mouseleave(function () {
        $(this).find('.bubble').fadeOut(200);
    });
</script>
</body>
