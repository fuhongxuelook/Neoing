<script>
    var price = $('.price');
    $.each(price, function (index, dom) {
        var ee = $(dom).html();
        if (ee == '-1.00') {
            $(dom).empty();
            $(dom).html('议价');
        } else if (ee == '-2.00') {
            $(dom).empty();
            $(dom).html('非卖品');
        } else if (ee == '-3.00') {
            $(dom).empty();
            $(dom).html('已售');
        } else if (ee == '-4.00') {
            $(dom).empty();
            $(dom).html('展示');
        }
    });
    $(function () {
        var askAnswer_item = $('.askAnswer-item');
        for (var i = 0; i < askAnswer_item.length; i++) {
            var ss;
            var is_anonymous = $('.is_anonymous').eq(i).val();
            if (is_anonymous == 1) {
                var anonymous = $('.is_anonymous').eq(i).siblings('.anonymous_name').html();
                var aaaa = anonymous.length - 2;
                var eee = anonymous.substring(2, aaaa);
                ss = anonymous.replace(eee, '');
                $('.is_anonymous').eq(i).siblings('.anonymous_name').html(ss);
            }
        }

    });
    $('.ui-dialog-submit').click(function () {
        var anony;
        var anonymous = $('.anonymous').attr("checked");
        if (anonymous) {
            anony = 1;
        } else {

        }
        var div_textarea = $(".div_textarea").val();
        var gid = $(".gid").val();
        var shop_id = $(".shop_id").val();
        $.ajax({
            url: '../../customer/informationAdvice',
            type: 'POST',
            data: {content: div_textarea, is_anonymous: anony, gid: gid, shop_id: shop_id},
            dataType: 'json',
            success: function (data) {
                if (data.status = 1) {
                    alert(data.msg);
                    $(".ui-dialog").hide();
                    $(".ui-mask").hide();
                } else {
                    alert(data.msg);
                    return false;
                }

            }
        })
    });
    $('.questions').click(function () {
        $(".ui-dialog").show(100);
        $(".ui-mask").show(100);
    });
    $('.ui-dialog-cancel,.ui-dialog-title_p1').click(function () {
        $(".ui-dialog").hide();
        $(".ui-mask").hide();
    });

    $(function () {
        var imgItem = $('#imgCon').find('img');
        var loadImg = function () {
            var imgObj = document.getElementsByTagName('img'),
                imgCount = imgObj.length;
            return {
                loadImgLoop: function loadImgLoop() {
                    var _loop = function _loop(i) {
                        var img = new Image();
                        var loadSrc = imgObj[i].getAttribute('data-loadSrc');

                        if (loadSrc != null) {
                            img.src = loadSrc;
                            img.onload = function () {
                                imgObj[i].src = this.src;
                            };
                        }
                    };

                    for (var i = 0; i < imgCount; i++) {
                        _loop(i);
                    }
                }
            };
        }();
        $.each(imgItem, function (index, ele) {
            var src = $(ele).attr('src'),
                count = src.indexOf('!');
            var loadSrc = src.substring(0, count);
            $(ele).attr('data-loadSrc', loadSrc);
        })

        loadImg.loadImgLoop();
    })
</script>
