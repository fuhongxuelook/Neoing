<script>

    var details1Name = $('.details1Name').html();
    var size = $('.data-size').html();
    var kuan = $('.kuan').html();
    if (size == ' *') {
        $('.data-size').hide()
    }
    if (kuan == '') {
        $('.aaa').hide();
    }
    if (details1Name == '') {
        $('.details1Name').parent('p').hide();
    }
    $('#type_details2').on('change', function () {
        var type_details2 = $("#type_details2").val();
        var gid = $(".gid").val();
        $.ajax({
            type: "POST",
            url: '/Home/Goods/goodsinfo',
            data: {type_details2: type_details2, gid: gid},
            success: function (data) {
                var jsonText = JSON.parse(data);
                if (jsonText.status == 1) {
                    var url = jsonText.msg;
                    window.location.href = url;
                } else {

                }
            },
        });
    });

    $('.box .div_head').eq(0).show();
    $('.ul1 li').click(function () {
        var index = $(this).index();
        $(this).addClass('active').siblings().removeClass('active');
        $('.box .div_head').eq(index).show().siblings().hide();
    });
    $('#type_details1').on('change', function () {
        var type_details1 = $("#type_details1").val();
        var gid = $(".gid").val();
        $.ajax({
            type: "POST",
            url: '/Home/Goods/goodsinfo',
            data: {type_details1: type_details1, gid: gid},
            success: function (data) {
                var jsonText = JSON.parse(data);
                if (jsonText.status == 1) {
                    var url = jsonText.msg;
                    window.location.href = url;
                } else {

                }
            }
        });
    })
</script>

