                        <p style=" margin-top: -16px;">那些年</p>
                        <p>
                        <p class="goodes_data_p">￥398.00</p><input style="display: none"
                                                                                     value="398.00"
                                                                                     class="goods_data_price"></p>
                        <script>
                            var goods_data_price = $('.goods_data_price').val();
                            if (goods_data_price == '-1.00') {
                                $('.goodes_data_p').empty();
                                $('.goodes_data_p').html('议价')
                            } else if (goods_data_price == '-2.00') {
                                $('.goodes_data_p').empty();
                                $('.goodes_data_p').html('非卖品')
                            } else if (goods_data_price == '-3.00') {
                                $('.goodes_data_p').empty();
                                $('.goodes_data_p').html('已售')
                            } else if (goods_data_price == '-4.00') {
                                $('.goodes_data_p').empty();
                                $('.goodes_data_p').html('展示')
                            }
                        </script>
                        <p style="color: #ff783e;font-size: 16px;">
